<%- include ('_header') -%>
<div class="u-section-6">
<div class="container pt-5 pb-5" >
  <div class="row">
    <div class="col-6"><h1>Booked Places</h1></div>
</div>

    <table class="table table-bordered table-striped"  >
        <thead class="thread-dark">
          <tr>
            <th scope="col">#</th>
            <th scope="col">Place ID</th>
            <th scope="col">Number of Guests</th>
            <th scope="col">Number of Guests</th>
            <th scope="col">Check In Date</th>
            <th scope="col">Check Out Date</th>
            <th scope="col">Status</th>
          </tr>
        </thead>
        <tbody>
            <% if(data.length){
 
                for(var i = 0; i< data.length; i++) {%>  
          <tr>
            <th scope="row"><%= (i+1) %></th>
            <td><%= data[i].place_id %></td>
            <td><%= data[i].num_guests %></td>
            <td><%= data[i].num_nights %></td>
            <td><%= data[i].checkin_date%></td>
            <td><%= data[i].checkin_date %></td>
            <td><%= data[i].status %></td>
          </tr>
          <% }
           
   }else{ %>
       <tr>
          <td colspan="3">No Booking</td>
       </tr>
    <% } %> 

        </tbody>
      </table>
</div>
</div>
<%- include ('_footer') -%>