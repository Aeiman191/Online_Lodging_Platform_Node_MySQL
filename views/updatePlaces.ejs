<h2>Update Places</h2>
<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex-justify-between">
            <a href="/host" class="border-shadow">
                <span class="text-gradient">New Place<i class="fa-solid fa-house"></i></span>
            </a>
        </div>

        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    
                    <tr>
                        <th>Property Name</th>
                        <th>Property Type</th>
                        <th>Space Type</th>
                        <th>Number of Guests</th>
                        <th>Number of Bedrooms</th>
                        <th>Number of Bathrooms</th>
                        <th>Price</th>
                        <th>Action</th>
                    </tr>
                </thead>

                <tbody>
                    <% for(var i = 0;i < data.length; i++){%>
                    <tr>

                        <td><%= data[i].name %></td>
                        <td><%= data[i].property_type %></td>
                        <td><%= data[i].space_type %></td>
                        <td><%= data[i].num_guests %></td>
                        <td><%= data[i].num_beds %></td>
                        <td><%= data[i].num_baths %></td>
                        <td><%= data[i].price %></td>
                        <td>
                            <a href="/updatePlacesForm?id=<%= data[i].place_id %>" class="btn border-shadow update">
                                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
                            </a>
                            <a class="btn border-shadow delete">
                                <span class="text-gradient"><i class="fas fa-times"></i></span>
                            </a>
                        </td>

                    </tr>
                    <%} %>
                </tbody>
            </table>
        </form>
    </div>
</main>

</body>
